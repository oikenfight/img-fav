<div class="row">
<p id="notice"><%= notice %></p>
</div>
<% if @current_user %>
<div class="row">
    <%= link_to 'Destroy', @user, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-danger col-md-1 pull-right" %>
    <%= link_to 'Edit', edit_user_path(@user), class: "btn btn-warning col-md-1 pull-right" %>
</div>
<% end %>
<div class="row">
<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>
</div>

<%= render 'table', images: @my_images, title: '投稿画像一覧' %>

<%= render 'table', images: @fav_images, title: 'お気に入り一覧' %>